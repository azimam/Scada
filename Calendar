 	 	dec42 AT %M(.Calendar Link Points.12 December.42.NoteText) : STRING;
 	 	 
 	 	 
 	END_VAR	 

	
	VAR
	
		year AT %M(.Year.CurrentValue) : INT;
	
	END_VAR	 
		
	
	VAR
		jan : ARRAY [1..42] OF STRING;
		feb : ARRAY [1..42] OF STRING;
		mar : ARRAY [1..42] OF STRING;
		apr : ARRAY [1..42] OF STRING;
		may : ARRAY [1..42] OF STRING;
		jun : ARRAY [1..42] OF STRING;
		jul : ARRAY [1..42] OF STRING;
		aug : ARRAY [1..42] OF STRING;
		sep : ARRAY [1..42] OF STRING;
		oct : ARRAY [1..42] OF STRING;
		nov : ARRAY [1..42] OF STRING;
		dec : ARRAY [1..42] OF STRING;
		x : INT;
		count : INT;
		day_count : SINT;
		jan_first_day : INT;
		jan_last_day : INT;
		feb_last_day : INT;
		mar_last_day : INT;
		apr_last_day : INT;
		may_last_day : INT;
		jun_last_day : INT;
		jul_last_day : INT;
		aug_last_day : INT;
		sep_last_day : INT;
		oct_last_day : INT;
		nov_last_day : INT;
		dec_last_day : INT;
		
	
	END_VAR;	 	 	 
	
(* year = Y, m = month − 2 mod 12 (March = 1,..., January = −1 mod 12 = 11 and February = 12) and d = days of the month, this formula becomes
Gregorian Calender:	
w=MOD( d + [2.6m-.02] + 5*MOD(Y,4) + 4*MOD(Y,100) + 6*MOD(Y,400),7)

For the Julian calendar
w=MOD( d + [2.6m-.02] + 5*MOD(Y,4) + 3*MOD(Y,7) ,7)
Note: year − 1 for January and February.
For January 1, 2016, the date would be treated as the 11th month of 2015*)
	year:= EXTRACT_DT_YEAR(NOW());

	jan_first_day := MOD( 29 +  5*MOD(year-1,4) + 3*MOD(year-1,7)  ,7) ;
	
		
		
		FOR L := 1 TO jan_first_day DO
			jan[L] := ' ';
		END_FOR;
		
		count := 1;      	
		FOR N := jan_first_day TO 42 DO

			jan[N] := INT_TO_STRING(count);
			
			IF ( count = 32) THEN
			jan_last_day := N;
			END_IF;
			
			IF (count >= 32) THEN
			jan[N] := ' ';
			END_IF;
			count := count+1;
			
		END_FOR;
		
		IF (jan_last_day >=36 AND jan_last_day <=42) THEN
		IF(jan_last_day = 36) THEN
		jan_last_day := 8;
		
		ELSE
		jan_last_day := jan_last_day - 35;
		END_IF;
		
		ELSIF (jan_last_day >= 29 & jan_last_day <= 35) THEN
		jan_last_day := jan_last_day - 28;
		
		END_IF;
		
		(* END MONTH OF JANUARY*)
		
		count := 1;
		
		FOR L := 1 TO jan_last_day DO
			feb[L] := ' ';
		END_FOR;
		
		IF(year MOD 4 = 0) THEN
		
		FOR N := jan_last_day TO 42 DO
			feb[N] := INT_TO_STRING(count);
			
			IF (count = 30) THEN
			feb_last_day := N;
			END_IF;
			
			IF (count >= 30) THEN
			feb[N] := ' ';
			END_IF;
			
			count := count+1;
			
		END_FOR;
		
		ELSE
		
		FOR N := jan_last_day TO 42 DO
			feb[N] := INT_TO_STRING(count);
			
			IF (count = 29) THEN
			feb_last_day := N;
			END_IF;
			
			IF (count >= 29) THEN
			feb[N] := ' ';
			END_IF;
			
			count := count+1;
		END_FOR;
		
		END_IF;
		
		IF (feb_last_day >=36 AND feb_last_day <=42) THEN
		
		IF(feb_last_day = 36) THEN
		feb_last_day := 8;
		
		ELSE
		feb_last_day := feb_last_day - 35;
		END_IF;
		
		ELSIF (feb_last_day >= 29 AND feb_last_day <= 35) THEN
		feb_last_day :=  feb_last_day - 28;
		
		END_IF;
		
		(* END MONTH OF FEBRUARY*)
		
		count := 1;
		
		FOR L := 1 TO feb_last_day DO
			mar[L] := ' ';
		END_FOR;
		
		FOR N := feb_last_day TO 42 DO
			mar[N] := INT_TO_STRING(count);
			
			IF ( count = 32) THEN
			mar_last_day := N;
			END_IF;
			
			IF (count >= 32) THEN
			mar[N] := ' ';
			END_IF;
			
			count := count+1;
			
		END_FOR;
		
		
		IF (mar_last_day >=36 AND mar_last_day <=42) THEN
		
		IF(mar_last_day = 36) THEN
		mar_last_day := 8;
 
		ELSE
		mar_last_day := mar_last_day - 35;
		END_IF;
		
		ELSIF (mar_last_day >= 29 AND mar_last_day <= 35) THEN
		mar_last_day :=  mar_last_day - 28;
		
		END_IF;
		
		(* END MONTH OF MARCH*)
		
		count := 1;
		
		FOR L := 1 TO mar_last_day DO
			apr[L] := ' ';
		END_FOR;
		
		FOR N := mar_last_day TO 42 DO
			apr[N] := INT_TO_STRING(count);
			
			IF ( count = 31) THEN
			apr_last_day := N;
			END_IF;
			
			IF (count >= 31) THEN
			apr[N] := ' ';
			END_IF;
			
			count := count+1;
			
		END_FOR;
		
		IF (apr_last_day >=36 AND apr_last_day <=42) THEN
		IF(apr_last_day = 36) THEN
		apr_last_day := 8;
		 
		ELSE
		apr_last_day := apr_last_day - 35;
		END_IF;

		
		ELSIF (apr_last_day >= 29 AND apr_last_day <= 35) THEN
		apr_last_day :=  apr_last_day - 28;
		
		END_IF;
		
		(* END MONTH OF APRIL*)
		
		count := 1;
		
		FOR L := 1 TO apr_last_day DO
			may[L] := ' ';
		END_FOR;
		
		FOR N := apr_last_day TO 42 DO
			may[N] := INT_TO_STRING(count);
			
			IF ( count = 32) THEN
			may_last_day := N;
			END_IF;
			
			IF (count >= 32) THEN
			may[N] := ' ';
			END_IF;
			
			count := count+1;
			
		END_FOR;
		
		IF (may_last_day >=36 AND may_last_day <=42) THEN
		may_last_day := may_last_day - 35;
		
		ELSIF (may_last_day >= 29 AND may_last_day <= 35) THEN
		may_last_day :=  may_last_day - 28;
		
		END_IF;
		
		(* END OF MAY *)
		
		count := 1;
		
		FOR L := 1 TO may_last_day DO
			jun[L] := ' ';
		END_FOR;
		
		FOR N := may_last_day TO 42 DO
			jun[N] := INT_TO_STRING(count);
			
			IF ( count = 31) THEN
			jun_last_day := N;
			END_IF;
			
			IF (count >= 31) THEN
			jun[N] := ' ';
			END_IF;
			
			count := count+1;
			
		END_FOR;
		
		IF (jun_last_day >=36 AND jun_last_day <=42) THEN
		IF(jun_last_day = 36) THEN
		jun_last_day := 8;
		 
		ELSE
		jun_last_day := jun_last_day - 35;
		END_IF;
		
		ELSIF (jun_last_day >= 29 AND jun_last_day <= 35) THEN
		jun_last_day :=  jun_last_day - 28;
		
		END_IF;
		
		(* END OF JUNE*)
		
		count := 1;
		
		FOR L := 1 TO jun_last_day DO
			jul[L] := ' ';
		END_FOR;
		
		FOR N := jun_last_day TO 42 DO
			jul[N] := INT_TO_STRING(count);
			
			IF ( count = 32) THEN
			jul_last_day := N;
			END_IF;
			
			IF (count >= 32) THEN
			jul[N] := ' ';
			END_IF;
			
			count := count + 1;
			
		END_FOR;
		
		IF (jul_last_day >=36 AND jul_last_day <=42) THEN
		IF(jul_last_day = 36) THEN
		jul_last_day := 8;
		 
		ELSE
		jul_last_day := jul_last_day - 35;
		END_IF;

		
		ELSIF (jul_last_day >= 29 AND jul_last_day <= 35) THEN
		jul_last_day :=  jul_last_day - 28;
		
		END_IF;
		
		(*END OF JULY*)
		
		count := 1;	 
 	 
		FOR L := 1 TO jul_last_day DO	 
		 	aug[L] := ' ';
		END_FOR;	 
		 	 
		FOR N := jul_last_day TO 42 DO	 
		 	aug[N] := INT_TO_STRING(count);
		 	 
		 	IF ( count = 32) THEN
		 	aug_last_day := N;
		 	END_IF;
		 	 
		 	IF (count >= 32) THEN
		 	aug[N] := ' ';
		 	END_IF;
		 	 
		 	count := count + 1;
		 	 
		END_FOR;	 
		 	 
		IF (aug_last_day >=36 AND aug_last_day <=42) THEN	 
		IF(aug_last_day = 36) THEN
		aug_last_day := 8;
		 
		ELSE
		aug_last_day := aug_last_day - 35;
		END_IF;
	 
		 	 
		ELSIF (aug_last_day >= 29 AND aug_last_day <= 35) THEN	 
		aug_last_day := aug_last_day - 28;	 
		 	 
		END_IF;	 
		
		(*END OF AUGUST*)	
		
		count := 1;	 
 	 
		FOR L := 1 TO aug_last_day DO	 
		 	sep[L] := ' ';
		END_FOR;	 
		 	 
		FOR N := aug_last_day TO 42 DO	 
		 	sep[N] := INT_TO_STRING(count);
		 	 
		 	IF ( count = 31) THEN
		 	sep_last_day := N;
		 	END_IF;
		 	 
		 	IF (count >= 31) THEN
		 	sep[N] := ' ';
		 	END_IF;
		 	 
		 	count := count+1;
		 	 
		END_FOR;	 
		 	 
